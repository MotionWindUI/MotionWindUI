{
  "name": "@motionwindui/{{componentName}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "keywords": [
    "{{componentName}}"
  ],
  "author": "Kyle Gagnon <kmgagnon99@gmail.com>",
  "license": "ISC",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  }
  "files": [
    "dist/"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MotionWindUI/MotionWindUI",
    "directory": "packages/{{outDir}}/{{componentName}}"
  },
  "bugs": {
    "url": "https://github.com/MotionWindUI/MotionWindUI/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean:build": "rm -rf dist .turbo",
    "clean:node_modules": "rm -rf node_modules",
    "clean": "pnpm clean:build && pnpm clean:node_modules",
    "typeCheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint-fix": "eslint . --fix"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-aria": "^3.0.0"
    "react-dom": ">=18",
    "@motionwindui/theme": ">=1.0.0",
  },
  "devDependencies": {
    "@motionwindui/eslint-config": "workspace:*",
    "@motionwindui/typescript-config": "workspace:*",
    "@motionwindui/theme": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "react-aria": "catalog:"
  }
}
