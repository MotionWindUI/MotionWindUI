{
  "name": "@motionwindui/{{componentName}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "keywords": [
    "{{componentName}}"
  ],
  "author": "Kyle Gagnon <kmgagnon99@gmail.com>",
  "license": "ISC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MotionWindUI/MotionWindUI",
    "directory": "packages/{{outDir}}/{{componentName}}"
  },
  "bugs": {
    "url": "https://github.com/MotionWindUI/MotionWindUI/issues"
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "concurrently \"vite build --watch\" \"tsc --emitDeclarationOnly --watch\"",
    "clean": "rm -rf dist .turbo",
    "typeCheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
  }
}
