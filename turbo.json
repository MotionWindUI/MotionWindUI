{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "cache": false,
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build", "lint", "^test"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "typeCheck": {
      "cache": false,
      "dependsOn": ["^typeCheck"]
    },
    "clean": {
      "dependsOn": ["^clean"]
    }
  }
}
