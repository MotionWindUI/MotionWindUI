{
  "name": "@motionwindui/theme",
  "version": "1.0.0",
  "description": "The theme configuration / TailwindCSS plugin for MotionWindUI",
  "keywords": [
    "theme"
  ],
  "author": "Kyle Gagnon <kmgagnon99@gmail.com>",
  "license": "ISC",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MotionWindUI/MotionWindUI",
    "directory": "packages/core/theme"
  },
  "bugs": {
    "url": "https://github.com/MotionWindUI/MotionWindUI/issues"
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "concurrently \"vite build --watch\" \"tsc --emitDeclarationOnly --watch\"",
    "clean": "rm -rf dist",
    "typeCheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "@types/node": "catalog:",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:"
  }
}
