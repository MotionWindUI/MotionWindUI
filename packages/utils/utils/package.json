{
  "name": "@motionwindui/utils",
  "version": "1.0.0",
  "description": "General utility functions used by MotionWindUI",
  "keywords": [
    "utils"
  ],
  "author": "Kyle Gagnon <kmgagnon99@gmail.com>",
  "license": "ISC",
  "sideEffects": false,
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MotionWindUI/MotionWindUI",
    "directory": "packages/utils/utils"
  },
  "bugs": {
    "url": "https://github.com/MotionWindUI/MotionWindUI/issues"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean:build": "rm -rf dist .turbo",
    "clean:node_modules": "rm -rf node_modules",
    "clean": "pnpm clean:build && pnpm clean:node_modules",
    "typeCheck": "tsc --noEmit",
    "lint": "eslint . --max-warnings 0",
    "lint-fix": "eslint . --fix"
  },
  "devDependencies": {
    "@motionwindui/eslint-config": "workspace:*",
    "@motionwindui/typescript-config": "workspace:*",
    "@types/node": "catalog:"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2"
  }
}
